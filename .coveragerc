# Coverage configuration for Foundations & Guards service

[run]
source = src
omit = 
    */tests/*
    */test_*
    */conftest.py
    */venv/*
    */env/*
    */.venv/*
    */migrations/*
    */alembic/*
    setup.py
    */setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

ignore_errors = True
show_missing = True
precision = 2

[html]
directory = htmlcov
title = Foundations & Guards Service Coverage Report

[xml]
output = coverage.xml
